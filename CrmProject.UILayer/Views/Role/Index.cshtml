@model List<AppRole>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_CrmLayout.cshtml";
}

<h1>Rol Listesi</h1>

<br />
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Rol</th>

    </tr>
   
    @foreach (var item in Model)
    {<tr>
          <td>@item.Id</td>
          <td>@item.Name</td>
     </tr>
    }

</table>
<a href="/Role/AddRole/" class="btn btn-outline-primary">Rol Ekle</a>